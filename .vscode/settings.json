// Place your settings in this file to overwrite default and user settings.
{
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.rulers": [120],
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.pyc": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "lldb.logLevel": 30,
    "lldb.loggers": {
        "wireprotocol": 0
    },
    "python.linting.enabled": false
}
